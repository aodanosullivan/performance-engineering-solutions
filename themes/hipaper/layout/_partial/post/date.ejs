<% if (!is_current("about", false)){ %>
	<a href="/team/" class="<%= class_name %>">
          <%- post.author %> &nbsp;
	</a>
	<a href="<%- url_for(post.path) %>" class="<%= class_name %>">
	  <time datetime="<%= date_xml(post.date) %>" itemprop="datePublished">[<%= date(post.date, 'MMMM D, YYYY') %>]</time>
	</a>
<% } %>
